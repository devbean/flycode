<idea-plugin>
    <id>org.galaxyworld.flycode</id>
    <name>FlyCode</name>
    <vendor email="support@galaxyworld.org" url="http://www.galaxyworld.org">GalaxyWorld</vendor>

    <description><![CDATA[
      Plugin used for generating code from database schema and Java code.
    ]]></description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.java</depends>
    <depends optional="true" config-file="withDatabase.xml">com.intellij.database</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>

    </actions>
</idea-plugin>
